<!--post-->
<section class="contact">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading text-uppercase">YOUR LIKES</h2>
      </div>
    </div>

    <div class="row">
    <% @likes_posts.reverse_each do |like_post| %>
      <div class="col-md-12 col-lg-6">
        <div class='music-card'>
          <div class='image'>
            <%= link_to image_tag(like_post.image), like_post %>
          </div>
          <div class='wave'></div>
          <div class='wave'></div>
          <div class='wave'></div>
          <div class='info'>
            <h2 class='title'>by <br><%= like_post.user.name %></h2>
          </div>
        </div>
        <hr>
      </div>
    <% end %>
    </div>
  </div>
</section>